RewriteEngine  on

# next 2 lines prevent internal redirects that apply the rules twice
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-f

RewriteRule ^.*?/(.*)$ $1  [NC]   #kih this rule will strip the first segment of the path that follows the domain because it is the db name. example: catalogit.cleverbucket.com/dbname/index.php

